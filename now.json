{
  "version": 2,
  "name": "me",
  "builds": [{
      "src": "/static/*",
      "use": "@now/static"
    },
    {
      "src": "package.json",
      "use": "@now/next"
    }
  ],
  "routes": [{
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/blog/(?<slug>.+)",
      "dest": "/post?id=$slug"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
